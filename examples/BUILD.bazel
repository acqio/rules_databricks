load("//:index.bzl", "databricks_configure", "databricks_fs", "databricks_libraries")

databricks_configure(
    name = "cfg",
    cluster_name = "{AZ_DEV_DF_CLUSTER_NAME}",
    profile = "{AZ_DEV_DF_CLUSTER_PROFILE}",
)

databricks_fs(
    name = "fs",
    configure = ":cfg",
    files = ["dummy.jar"],
    stamp = "{STABLE_GIT_DATE}",
)

databricks_libraries(
    name = "lib",
    configure = ":cfg",
    dbfs = ":fs.cp",
)
